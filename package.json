{
  "name": "extended-node-social-app",
  "version": "1.0.0",
  "description": "A basic social media app built with Node.js, Fastify, TypeScript, Prisma, and PostgreSQL.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/views/*.hbs src/views/partials/*.hbs src/**/*.css dist/",
    "build": "npm run clean && tsc && npm run copy-files",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "Kristian Coulson",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/cookie": "^9.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/passport": "^2.4.0",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^6.12.0",
    "@fastify/view": "^8.2.0",
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.1.0",
    "ejs": "^3.1.9",
    "fastify": "^4.25.2",
    "passport-local": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.10.6",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2",
    "prettier": "3.2.2",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "vitest-mock-extended": "^1.3.1"
  }
}
